        android:layout_marginTop="48dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textAllCaps="false"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/w_el12"
        android:layout_width="51dp"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="47dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textAllCaps="false"
        app:layout_constraintLeft_toRightOf="@+id/w_el11"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/w_el13"
        android:layout_width="51dp"
        android:layout_height="40dp"
        android:layout_marginTop="47dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textAllCaps="false"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/w_el12"
        android:layout_marginLeft="8dp" />

    <EditText
        android:id="@+id/w_el21"
        android:layout_width="51dp"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textAllCaps="false"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/w_el11" />

    <EditText
        android:id="@+id/w_el22"
        android:layout_width="51dp"
        android:layout_height="38dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textAllCaps="false"
        app:layout_constraintLeft_toRightOf="@+id/w_el21"
        app:layout_constraintTop_toBottomOf="@+id/w_el12" />

    <EditText
        android:id="@+id/w_el23"
        android:layout_width="51dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textAllCaps="false"
        app:layout_constraintTop_toBottomOf="@+id/w_el13"
        app:layout_constraintLeft_toRightOf="@+id/w_el22"
        android:layout_marginLeft="8dp" />

    <EditText
        android:id="@+id/w_el31"
        android:layout_width="51dp"
